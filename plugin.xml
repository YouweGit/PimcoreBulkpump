<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
    <plugin>

        <!-- the plugin name [A-Za-z_] -->

        <pluginName>PimcoreBulkpump</pluginName>

        <!-- a short description -->
        <pluginDescription><![CDATA[Add functionality for import from files (GUI/CLI) ]]></pluginDescription>

        <!-- meta data -->
        <pluginVersion>0.4.14</pluginVersion>
        <pluginRevision>1</pluginRevision>
        <pluginBuildTimestamp>0</pluginBuildTimestamp>

        <!-- put in the URL for the plugin configuration eg. /plugin/CsvImport/admin/index -->
        <pluginIframeSrc></pluginIframeSrc>

        <!-- className of the plugin which extends Pimcore_API_Plugin_Abstract-->
        <pluginClassName>\PimcoreBulkpump\Plugin</pluginClassName>

        <!-- include paths relative to plugin-directory -->
        <pluginIncludePaths>
            <path>/PimcoreBulkpump/controllers</path>
            <path>/PimcoreBulkpump/lib</path>
            <path>/PimcoreBulkpump/models</path>
        </pluginIncludePaths>

        <!-- namespaces to register with autoloader-->
        <pluginNamespaces>
            <namespace>PimcoreBulkpump</namespace>
            <namespace>BulkPump</namespace>
            <namespace>CsvImport</namespace>
            <namespace>CsvDataMapper</namespace>
        </pluginNamespaces>

        <!-- js files with path relative to plugin-directory -->
        <pluginJsPaths>


            <!--models-->
            <path>/plugins/PimcoreBulkpump/static/js/admin/profiles.js</path>
            <path>/plugins/PimcoreBulkpump/static/js/admin/object.js</path>
            <path>/plugins/PimcoreBulkpump/static/js/admin/columns.js</path>
            <path>/plugins/PimcoreBulkpump/static/js/admin/log.js</path>

            <!--panels-->
            <path>/plugins/PimcoreBulkpump/static/js/admin/panel/mainPanel.js</path>
            <path>/plugins/PimcoreBulkpump/static/js/admin/panel/tabPanel.js</path>
            <path>/plugins/PimcoreBulkpump/static/js/admin/panel/gridlist.js</path>
            <path>/plugins/PimcoreBulkpump/static/js/admin/panel/uploadForm.js</path>


            <!--tabs-->
            <path>/plugins/PimcoreBulkpump/static/js/admin/panel/tabPanel/profileTab.js</path>
            <path>/plugins/PimcoreBulkpump/static/js/admin/panel/tabPanel/configTab.js</path>
            <path>/plugins/PimcoreBulkpump/static/js/admin/panel/tabPanel/dataTab.js</path>
            <path>/plugins/PimcoreBulkpump/static/js/admin/panel/tabPanel/logsTab.js</path>
            <path>/plugins/PimcoreBulkpump/static/js/admin/panel/tabPanel/filterConfigWindow2.js</path>
            <path>/plugins/PimcoreBulkpump/static/js/admin/panel/tabPanel/paramsPanel.js</path>
            <path>/plugins/PimcoreBulkpump/static/js/startup.js</path>

        </pluginJsPaths>

        <!-- css files with path relative to plugin-directory -->
        <pluginCssPaths>
            <path>/plugins/PimcoreBulkpump/static/css/example.css</path>
        </pluginCssPaths>
    </plugin>
</zend-config>
